{
  "name": "loyalty",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "pod": "cd ios && pod install --repo-update && cd ..",
    "android-development": "react-native run-android",
    "android-staging": "ENVFILE=.env.staging react-native run-android",
    "build:android-staging": "cd android && ENVFILE=.env.staging ./gradlew assembleRelease",
    "push:android-production": "appcenter codepush release-react -a levinci/Jollibee-Comsumer-Android -d Production -t 1.0 --description test",
    "ios": "react-native run-ios --scheme='loyalty-develop' --simulator='iPhone 11 Pro' ",
    "ios-staging": "react-native run-ios --scheme='loyalty-staging' --simulator='iPhone 11 Pro' ",
    "ios-real-staging": "react-native run-ios --scheme='loyalty-staging'  --device 'iPhone' ",
    "clean": "rm -rf node_modules && watchman watch-del-all",
    "clean:ios": "cd ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && rm -rf Podfile.lock",
    "clean:android": "cd android && ./gradlew clean",
    "release:ios": "bundle exec fastlane ios release",
    "push:ios-production": "appcenter codepush release-react -a levinci/Jollibee-Consummer -d Production -t 1.0 --description fixbug",
    "push:ios-staging": "appcenter codepush release-react -a levinci/Jollibee-Consummer -d Staging -t 1.0 --description test"
  },
